# Database
DATABASE_URL=postgresql://user:password@localhost:5432/tetsuo_pool

# Redis
REDIS_URL=redis://:password@localhost:6379

# TETSUO RPC
TETSUO_RPC_HOST=127.0.0.1
TETSUO_RPC_PORT=8337
TETSUO_RPC_USER=user
TETSUO_RPC_PASS=password
TETSUO_RPC_WALLET=pool

# Pool Configuration
POOL_FEE_PERCENT=10
PPLNS_WINDOW_MINUTES=120
MIN_PAYOUT_THRESHOLD=100
BLOCK_MATURITY_CONFIRMATIONS=100
POOL_WALLET_ADDRESS=YOUR_POOL_WALLET_ADDRESS

# ckpool
CKPOOL_LOG_PATH=/var/log/ckpool/ckpool.log
